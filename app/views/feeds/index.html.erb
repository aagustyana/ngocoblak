<h1>Listing feeds</h1>

<table border="1">
  <tr>
    <th>Name</th>
    <th>Url</th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%=h feed.name %></td>
    <td><%=h feed.url %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New feed', new_feed_path %>